<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertBand</servlet-name>
        <servlet-class>servlets.InsertBand</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BandsByYear</servlet-name>
        <servlet-class>servlets.BandsByYear</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllBands2</servlet-name>
        <servlet-class>servlets.AllBands2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RankingBands</servlet-name>
        <servlet-class>servlets.RankingBands</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SongsByLength</servlet-name>
        <servlet-class>servlets.SongsByLength</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RankingSongs</servlet-name>
        <servlet-class>servlets.RankingSongs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RankingAlbums</servlet-name>
        <servlet-class>servlets.RankingAlbums</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertBand</servlet-name>
        <url-pattern>/InsertBand</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BandsByYear</servlet-name>
        <url-pattern>/BandsByYear</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllBands2</servlet-name>
        <url-pattern>/AllBands2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RankingBands</servlet-name>
        <url-pattern>/RankingBands</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SongsByLength</servlet-name>
        <url-pattern>/SongsByLength</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RankingSongs</servlet-name>
        <url-pattern>/RankingSongs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RankingAlbums</servlet-name>
        <url-pattern>/RankingAlbums</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
